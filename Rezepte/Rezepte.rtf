lain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Erdbeeren, schwarze Johannisbeeren, Heidelbeeren, Himbeeren, Blaubeeren{\fs24\par}}\pard\plain\sb346\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1  {\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1 Glasur:{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1 Päckchen\plain\tab\fs22\f2\cf0\cb1 roter Tortenguss{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 2 EL\plain\tab\fs22\f2\cf0\cb1 Akazienhonig{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1/4 l\plain\tab\fs22\f2\cf0\cb1 Johannisbeersaft oder Rotwein{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1  {\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1 Karamellhaube:{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 100 g\plain\tab\fs22\f2\cf0\cb1 Zucker{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1 EL\plain\tab\fs22\f2\cf0\cb1 Olivenöl {\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1 gehackte Pistazien für die Dekoration{\fs27\par}}{\page}\pard\plain\tx90\tqr\tx10227\tqr\tx10317{\plain\tab\fs40\f1\cf0\cb1 Beerentraumtorte\plain\tab\fs22\f2\cf0\cb1 Kuchen\plain\tab\fs22\f2\cf0\cb1 Gaby Heyne aus {\fs56\par}}\pard\plain\tqr\tx10215\tqr\tx10305{\plain\tab\fs22\f2\cf0\cb1 Greven-Reckenfeld\plain\tab\fs22\f2\cf0\cb1  {\fs32\par}}\pard\plain\sb197\tx1673{\plain\tab\fs28\f3\cf0\cb1 Zubereitung{\fs34\par}}\pard\plain\sb136\tx1673{\plain\tab\fs22\f2\cf0\cb1 Zubereitung Biskuitboden: {\fs27\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Eine Tortenbodenform (28 cm) mit Butter einfetten und mit Mehl gleichmäßig {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 bestäuben. Die Eier trennen und das Eiweiß steif schlagen. Das Eigelb mit dem Honig {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 schaumig schlagen. Mehl mit Backpulver vermengen und nach und nach in die lockere {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Eiermasse rühren. Das kalte Wasser dazugeben und mit dem Mixer auf Stufe drei {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 schaumig rühren. Den Eischnee vorsichtig unterheben.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Den Teig in die Tortenbodenform füllen und im vorgeheizten Backofen auf mittlerer {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Schiene bei 200 Grad 15 Minuten backen.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Zubereitung Füllung: {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Sahne und Vanille steif schlagen und in den Kühlschrank stellen. Schmant und Honig {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 mit dem Mixer drei Minuten lang gut mischen und die Gelatine nach Packungsangabe {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 verrühren. Die steif geschlagene Sahne vorsichtig unter- heben. Die gemahlenen {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Haselnüsse frisch oder kurz in der Pfanne angeröstet auf dem abgekühlten {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Tortenboden gleichmäßig verteilen.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Einen Tortenring um den Boden fixieren, dann die gekühlte Schmant-Sahne-Füllung {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 vorsichtig einfüllen und gleichmäßig mit einem Teigschaber verteilen.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Die tiefgefrorenen Früchte darauf legen und den heißen Tortenguss sofort mit dem {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Esslöffel zügig von der Mitte aus verteilen und fest werden lassen. Die fertige Torte {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 sechs Stunden in den Kühlschrank stellen, damit sie fest wird.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Zubereitung Karamellhaube: {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Einen Esslöffel Olivenöl und den Zucker in einen Topf geben und langsam erhitzen, bis {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 der Zucker karamellisiert.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Die Temperatur so wählen, dass die Masse zähflüssig ist (falls sie zu flüssig, lassen {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 sich damit keine Fäden ziehen ist). Ein rundes Glasschälchen (10-15 cm Durchmesser){\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1  mit der Öffnung nach unten auf den Tisch stellen und mit Alufolie beschichten {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 (glänzende Seite der Folie nach außen).{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Ein paar Tropfen Öl auf der Folie verteilen und mit einem Löffel die zähflüssige {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Karamellmasse aus dem Topf nehmen und langsam vom Löffel auf die Folie laufen {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 lassen, so dass sich Fäden bilden.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Das geht nicht in einem Arbeitsgang, da sich die Masse auf dem Löffel abkühlt und {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 wieder erwärmt werden muss. Den Vorgang so lange wiederholen, bis sich ein dichtes {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Gespinst über die Folie gelegt hat.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Nach dem Abkühlen die Karamellhalbkugel vorsichtig von der Schale mit der Alufolie {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 lösen und kurz vor dem Servieren auf den Kuchen stellen.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Zum Schluss den Kuchen mit gehackten Pistazien garnieren.{\fs24\par}}\pard\plain\sb996\tx1673\tx6349{\plain\tab\fs22\f2\cf0\cb1 Guten Appetit!\plain\tab\fs22\f2\cf0\cb1 gesammelt von Gerlinde Fischer{\fs27\par}}{\page}\pard\plain\tx90\tqr\tx10227\tqr\tx10317{\plain\tab\fs40\f1\cf0\cb1 Belgischer Schokoladenkuchen\plain\tab\fs22\f2\cf0\cb1 Kuchen\plain\tab\fs22\f2\cf0\cb1  {\fs56\par}}\pard\plain\tqr\tx10215{\plain\tab\fs22\f2\cf0\cb1  {\fs27\par}}\pard\plain\tx1673{\plain\tab\fs28\f3\cf0\cb1 Zutaten  {\fs34\par}}\pard\plain\sb79\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 300 g\plain\tab\fs22\f2\cf0\cb1 Mehl{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 300 g\plain\tab\fs22\f2\cf0\cb1 Zucker{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 125 g\plain\tab\fs22\f2\cf0\cb1 Butter{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 4\plain\tab\fs22\f2\cf0\cb1 Eier{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 100 g\plain\tab\fs22\f2\cf0\cb1 Kakaopulver{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 200 g\plain\tab\fs22\f2\cf0\cb1 Kefir{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 100 g\plain\tab\fs22\f2\cf0\cb1 Kokosflocken{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 200 ml\plain\tab\fs22\f2\cf0\cb1 heißes Wasser{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1 TL\plain\tab\fs22\f2\cf0\cb1 Vanillinzucker{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1 Packung \plain\tab\fs22\f2\cf0\cb1 Backpulver{\fs27\par}}\pard\plai