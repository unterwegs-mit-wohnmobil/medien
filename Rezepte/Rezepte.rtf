age}\pard\plain\tx90\tqr\tx10227\tqr\tx10317{\plain\tab\fs40\f1\cf0\cb1 Hähnchenbrüste mit Safran und \plain\tab\fs22\f2\cf0\cb1 Hauptgericht\plain\tab\fs22\f2\cf0\cb1  {\fs56\par}}\pard\plain\tx90\tqr\tx10215{\plain\tab\fs40\f1\cf0\cb1 Diamantenreis Javaheri-Polo\plain\tab\fs22\f2\cf0\cb1  {\fs44\par}}\pard\plain\tx1673{\plain\tab\fs28\f3\cf0\cb1 Zutaten für 5 Personen{\fs34\par}}\pard\plain\sb79\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 5\plain\tab\fs22\f2\cf0\cb1 Hähnchenbrustfilets{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1,5 TL\plain\tab\fs22\f2\cf0\cb1 Safranpulver (Safranfäden mit 1 Stück Würfelzucker im Mörser zerkleinern){\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1\plain\tab\fs22\f2\cf0\cb1 Zitrone{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1 Salz{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1 Pfeffer{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1 TL\plain\tab\fs22\f2\cf0\cb1 Kurkuma{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 5\plain\tab\fs22\f2\cf0\cb1 Möhren, in Stifte geschnitten{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 4 - 5 Tassen\plain\tab\fs22\f2\cf0\cb1 Basmati-Reis{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1 Tasse\plain\tab\fs22\f2\cf0\cb1 Sereschk (Berberitzen){\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1 Tasse\plain\tab\fs22\f2\cf0\cb1 kandierte Orangenschalen{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1 Tasse\plain\tab\fs22\f2\cf0\cb1 gelbe Rosinen{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1/2 Tasse\plain\tab\fs22\f2\cf0\cb1 Mandelstifte{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1/2 Tasse\plain\tab\fs22\f2\cf0\cb1 Pistazien{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1 Zucker{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1  \plain\tab\fs22\f2\cf0\cb1 Öl oder Butter{\fs27\par}}{\page}\pard\plain\tx90\tqr\tx10227\tqr\tx10317{\plain\tab\fs40\f1\cf0\cb1 Hähnchenbrüste mit Safran und \plain\tab\fs22\f2\cf0\cb1 Hauptgericht\plain\tab\fs22\f2\cf0\cb1  {\fs56\par}}\pard\plain\tx90\tqr\tx10215{\plain\tab\fs40\f1\cf0\cb1 Diamantenreis Javaheri-Polo\plain\tab\fs22\f2\cf0\cb1  {\fs44\par}}\pard\plain\sb149\tx1673{\plain\tab\fs28\f3\cf0\cb1 Zubereitung{\fs34\par}}\pard\plain\sb136\tx1673{\plain\tab\fs22\f2\cf0\cb1 Den Reis in einen großen Topf füllen und 3-mal gut waschen. Anschließend mit {\fs27\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 reichlich Salzwasser zum Kochen bringen. Nach ca. 10 Minuten, wenn das Reiskorn gut{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1  aufgegangen ist und nur noch der Kern hart ist, den Reis in ein Sieb gießen und mit {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 kaltem Wasser abschrecken, damit die Reisstärke komplett heraus gewaschen ist. Den {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Topf mit Öl oder Butter (je nach Geschmack) bedecken und den Reis locker {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 aufschichten (es muss wie ein Berg oben spitz zulaufen). Dann mit einem Kochlöffel 4 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Löcher bis zum Boden drücken, damit die Hitze von unten aufsteigt. Nach ca. 5 Minuten{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1  den Reis bei starker Hitze anrösten, dann 1 Glas Wasser darüber verteilen. {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Topfdeckel mit einem Küchenhandtuch umwickeln und den Topf luftdicht verschließen, {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 damit der Dampf im Topf bleibt. Die Hitze jetzt ganz stark reduzieren.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Nach ca. 20 Minuten ist der Reis fertig. Um das Tadig (angebratener Reis) gut vom {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Topf lösen zu können, sollte man das Waschbecken mit etwas kaltem Wasser (3 cm) {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 füllen und den heißen Topf hineinstellen. Wenn es ein zischendes Geräusch gibt, ist es{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1  perfekt.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Die Hähnchenbrüste von allen Sehnen befreien. Safranpuder mit etwas kochendem {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Wasser in einem Glas übergießen und die Hälfte von dem Sud in eine Schale geben. {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Nun Saft einer Zitrone, 1 TL Kurkuma, Salz und Pfeffer zufügen. Dann die Hähnchen in {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 diesem Sud marinieren und mindestens 1 Stunde im Kühlschrank ziehen lassen.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Öl in eine Pfanne geben, die Hähnchenbrüste von beiden Seiten scharf anbraten und {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 anschließend bei größter Hitze ca. 6 Minuten im Backofen nachgrillen.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Für die kandierten Wurzelspalten die Möhrenstifte in Wasser etwas blanchieren, {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 danach im Topf mit Butter anbraten, und Zucker zum Kandieren zugeben. Zum Schluss {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 mit dem restlichen Safranwasser aufkochen.{\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Zum Servieren den Reis auf einer Platte anrichten. Die Pistazien und Mandeln rösten. {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Die Sereschk waschen, in einem kleinen Topf mit Butter anbraten, Zucker zum {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Kandieren dazugeben und im Anschluss einen Teil von dem Safranwasser dazugeben. {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Das Gleiche mit den gelben Rosinen machen. Den Reis abwechselnd mit den {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Berberitzen, Pistazien, kandierten Orangenschalenstiften, Mandeln und den gelben {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 Rosinen dekorieren. Auf einem separaten Teller die Hähnchen mit den Wurzelstiften {\fs24\par}}\pard\plain\tx1673{\plain\tab\fs22\f2\cf0\cb1 anrichten und mit etwas Sereschk, Mandeln und Pistazien garnieren.{\fs24\par}}\pard\plain\sb947\tx1673\tx6349{\plain\tab\fs22\f2\cf0\cb1 Guten Appetit!\plain\tab\fs22\f2\cf0\cb1 gesammelt von Gerlinde Fischer{\fs27\par}}{\page}\pard\plain\tx90\tqr\tx10227\tqr\tx10317{\plain\tab\fs40\f1\cf0\cb1 Hähnchenbrust-Paprika-Auflauf\plain\tab\fs22\f2\cf0\cb1 Hauptgericht\plain\tab\fs22\f2\cf0\cb1 Horst Lichter{\fs56\par}}\pard\plain\tqr\tx10215{\plain\tab\fs22\f2\cf0\cb1  {\fs27\par}}\pard\plain\tx1673{\plain\tab\fs28\f3\cf0\cb1 Zutaten für 4 Personen{\fs34\par}}\pard\plain\sb79\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 4\plain\tab\fs22\f2\cf0\cb1 Hähnchenbrustfilets{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1\plain\tab\fs22\f2\cf0\cb1 Paprika rot{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1\plain\tab\fs22\f2\cf0\cb1 Paprika gelb{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1\plain\tab\fs22\f2\cf0\cb1 Paprika grün{\fs27\par}}\pard\plain\sb40\tqr\tx1541\tx1673{\plain\tab\fs22\f2\cf0\cb1 1\plain\tab\fs22\f2\cf0\cb1 Zwiebel rot{\fs27\par}}\pard\plain\sb40\tqr\tx1541\t